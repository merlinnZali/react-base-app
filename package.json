{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.1",
    "@reduxjs/toolkit": "^1.6.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.28",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "@types/react-redux": "^7.1.19",
    "@types/styled-components": "^5.1.15",
    "@types/webpack-env": "^1.16.3",
    "axios": "^0.23.0",
    "bootstrap": "^5.1.1",
    "bootstrap-icons": "^1.5.0",
    "connected-react-router": "^6.9.1",
    "history": "4.10.1",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-rc.0",
    "react-dom": "^17.0.2",
    "react-hot-loader": "3.1.1",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "scss-loader": "^0.0.1",
    "serve": "^12.0.1",
    "source-map-explorer": "^2.5.2",
    "styled-components": "^5.3.1",
    "ts-jest": "^27.0.7",
    "typescript": "^4.4.3",
    "typings-for-scss-modules-loader": "^1.5.1",
    "web-vitals": "^1.1.2",
    "yarn": "^1.22.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:serve": "npm run build && serve -s build",
    "build:profile": "npm run build --profile && serve -s build",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "test": "react-scripts test",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ./.eslintignore --cache-location ./.eslintcache src --cache --color",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ./.eslintignore --cache-location ./.eslintcache src --cache --color --quiet --fix",
    "format": "prettier --write src/*.{ts,tsx,js,jsx,scss,css,json} src/**/*.{ts,tsx,js,jsx,scss,css,json}",
    "isready": "npm run format && npm run lint-fix && npm run build",
    "coverage": "npm run test -- --coverage",
    "test:clean": "rimraf ./coverage"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "prettier --write src/*.{ts,tsx,js,jsx} src/**/*.{ts,tsx,js,jsx} --ignore-path ./.prettierignore",
      "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ./.eslintignore --cache-location ./.eslintcache src --cache --color --quiet --fix",
      "git add"
    ],
    "*.+(json|css|scss|md)": [
      "prettier --write --ignore-path ./.gitignore",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/scope-manager": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "husky": "^4.3.8",
    "lint-staged": "^11.2.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coverageReporters": [
      "json",
      "text",
      "html",
      "lcov"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.{js,jsx,ts,tsx}",
      "!src/**/*/types.ts",
      "!src/**/store.ts",
      "!src/index.tsx",
      "!src/serviceWorker.ts",
      "!<rootDir>/node_modules/",
      "!**/templates/**",
      "!**/template/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 50,
        "lines": 50,
        "statements": 50
      }
    }
  }
}
