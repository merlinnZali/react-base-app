{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^12.20.28",
    "@types/react": "^17.0.27",
    "@types/react-dom": "^17.0.9",
    "husky": "^7.0.2",
    "node-sass": "^6.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "scss-loader": "^0.0.1",
    "typescript": "^4.4.3",
    "typings-for-scss-modules-loader": "^1.5.1",
    "web-vitals": "^1.1.2",
    "yarn": "^1.22.15"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ./.eslintignore --cache-location ./.eslintcache src --cache --color",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ./.eslintignore --cache-location ./.eslintcache src --cache --color --quiet --fix",
    "format": "prettier --write src/*.{ts,tsx,js,jsx,scss,css,json}",
    "isready": "npm run format && npm run lint && npm run build"
  },
  "lint-staged_init": {
    "*.js.*": "eslint",
    "*.ts.*": "eslint"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks_init": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run test"
    },
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "pre-push": "npm run test"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@typescript-eslint/scope-manager": "^4.33.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-webpack-plugin": "^2.5.4",
    "lint-staged": "^11.2.2",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1"
  }
}
